{
  "id": "IDD_TD_<%= documentName.toUpperCase() %>_MAIN",
  "type": 71,
  "text": "<%= documentTitle %>",
  "name": "MainData",
  "size": 2,
  "x": 0,
  "y": 0,
  "width": 654,
  "height": 73,
  "child": true,
  "items": [
    {
      "id": "IDC_<%= documentName.toUpperCase() %>_DOCNO",
      "controlClass": "StringEdit",
      "binding": {
        "datasource": "<%= documentName %>.DocNo"
      },
      "type": "Edit",
      "width": 57,
      "height": 12,
      "autoHScroll": true,
      "controlCaption": "Document No.",
      "anchor": "COL1",
      "captionSize": 3,
      "controlSize": 4
    },
    {
      "id": "IDC_<%= documentName.toUpperCase() %>_DOCDATE",
      "controlClass": "DateEdit",
      "binding": {
        "datasource": "<%= documentName %>.DocDate"
      },
      "type": "Edit",
      "width": 57,
      "height": 12,
      "autoHScroll": true,
      "controlCaption": "Document Date",
      "anchor": "COL1",
      "captionSize": 3,
      "controlSize": 3
    }
  ],
  "hasStaticArea": true,
  "grid": "col col-md-6"
}